  "features": [
    {
      "name": "Primal Senses",
      "text": "You are proficient in Perception checks and have advantage on Perception checks."
    },
    {
      "name": "Horn Attack",
      "text": "You are proficient with your horns. You may make an unarmed attack dealing 1d6 + Might modifier piercing damage."
    },
    {
      "name": "Immovable",
      "text": "You have advantage on checks and saving throws to resist being shoved or knocked prone."
    },
    {
      "name": "Stonehide",
      "text": "Add your Endurance modifier as temporary hit points. You regain these points after a short rest."
    },
    {
      "name": "Soldier's Network",
      "text": "You have ties to local fighters, guards, and mercenaries who can offer basic shelter, news, or introductions."
    },
    {
      "name": "Hold the Line",
      "text": "When a hostile creature you can see moves within 5 ft of you, you may use your Reaction to make a melee attack. On a hit, the creature's speed becomes 0 until the end of your next turn."
    },
    {
      "name": "Read the Threat",
      "text": "At the start of combat, you may make an Insight check with advantage to identify the most dangerous visible enemy (TN 8+DR). On a success, your first attack against that enemy has advantage."
    },
    {
      "name": "Unyielding Focus",
      "text": "You have advantage on saves made to resist being knocked prone, disarmed, or shoved."
    },
    {
      "name": "Folk Hero",
      "text": "Common people will shelter and aid you. You can find a place to hide, rest, or recuperate among commoners, unless you have shown yourself to be a danger to them."
    },
    {
      "name": "Extra Attack (3)",
      "text": "You can attack twice when you take the Attack action (gained at level 3)."
    }
  ],
