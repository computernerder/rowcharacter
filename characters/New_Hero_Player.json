    {
      "name": "Darkvision",
      "text": "You can see in dim light within 60 feet as if it were bright light, and in darkness as if it were dim light."
    },
    {
      "name": "Stonecunning",
      "text": "You are proficient in History checks and gain advantage when related to stonework."
    },
    {
      "name": "Iron Stomach",
      "text": "You have resistance to poison damage."
    },
    {
      "name": "Mountain Resolve",
      "text": "You have resistance to non-magical cold damage. You are proficient in Survival checks and have advantage on Survival checks."
    },
    {
      "name": "Leather Guild Access",
      "text": "You may use guild workshops and request basic help with crafting or repairing leather items."
    },
    {
      "name": "Adrenal Surge",
      "text": "When you roll a natural 20 on a Might check or melee attack, gain temporary Health Points equal to your Might modifier."
    },
    {
      "name": "Relentless Drive",
      "text": "When an attack reduces you below half Health Points, you may make a reaction attack to an enemy within 5 feet of you. This does not have to be the same enemy that attacked you."
    },
    {
      "name": "Furious Momentum",
      "text": "When you reduce a creature to 0 LP, you may move up to 10 ft as part of the same action without provoking opportunity attacks."
    },
    {
      "name": "Wayfarer's Lore",
      "text": "You know a song, story, or saying from nearly every region. This grants advantage on Charisma checks when trying to fit in with unfamiliar cultures.",
      "source": "Background: Wanderer"
    }
