  "features": [
    {
      "name": "Primal Senses",
      "text": "You are proficient in Perception checks and have advantage on Perception checks."
    },
    {
      "name": "Horn Attack",
      "text": "You are proficient with your horns. You may make an unarmed attack dealing 1d6 + Might modifier piercing damage."
    },
    {
      "name": "Immovable",
      "text": "You have advantage on checks and saving throws to resist being shoved or knocked prone."
    },
    {
      "name": "Charging Step",
      "text": "As part of your attack action, when you move 20 ft straight toward a target, your next horn attack deals an additional 1d6 damage."
    },
    {
      "name": "Guild Membership",
      "text": "Your guild offers workspace, trade materials, and connections with other artisans."
    },
    {
      "name": "Natural Resilience",
      "text": "You gain advantage on Survival and Navigation checks in nonmagical terrain."
    },
    {
      "name": "Hunter's Reflex",
      "text": "When rolling Initiative, you may move up to 10 ft immediately before combat begins."
    },
    {
      "name": "Keen Eyes",
      "text": "You can identify creature tracks, terrain signs, or nearby movement with a Wisdom (Perception) check as a Bonus Action."
    },
    {
      "name": "Beast Companion",
      "text": "You may befriend a beast with a DR equal to your character level divided by four (rounded down). Command them as a bonus action; they take their turn immediately after yours in initiative."
    },
    {
      "name": "Tribal Hospitality",
      "text": "Among your kin or allied clans, you can expect food, shelter, and aid. Other nomadic peoples may recognize kinship and offer similar courtesy.",
      "source": "Background: Nomad"
    }
  ],
